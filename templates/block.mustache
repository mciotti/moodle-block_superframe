{{!
    @template block_superframe/block

    This template is for render block's content.

    Example context (json):
    {
        "heading": "My superframe",
        "url": "http://www.moodlebites.com",
        "height": "400",
        "width": "600"
    }
}}
<div>
    <h3>{{welcome}}</h3>
    {{#url}}
    <a href="{{{url}}}">{{text}}</a>
    {{/url}}
    {{#students}}
    <ul class="list-group ml-0 mt-2">
        <li class="list-group-item list-group-item-warning">{{.}}</li>
    </ul>
    {{/students}}
</div>